{
    "name": "understeam/yii2-command-scheduler",
    "description": "Yii2 extenstion for Command Bus commands scheduling",
    "type": "extension",
    "require": {
        "yiisoft/yii2": "^2.0",
        "mtdowling/cron-expression": "^1.1"
    },
    "suggest": {
        "trntv/yii2-command-bus": "^2"
    },
    "require-dev": {
        "trntv/yii2-command-bus": "^2",
        "codeception/codeception": "~2.1",
        "codeception/verify": "*",
        "codeception/specify": "*"
    },
    "license": "GPLv3",
    "authors": [
        {
            "name": "Anatoly Rugalev",
            "email": "anatoly.rugalev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "understeam\\scheduler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "understeam\\scheduler\\tests\\": "tests"
        }
    }
}
